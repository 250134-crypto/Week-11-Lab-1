#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, m;
    vector<vector<double>> a(n, vector<double>(m));
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < m; ++j)
            cin >> a[i][j];

    for (int i = 0; i < n; ++i) {
        double sum = 0;
        for (int j = 0; j < m; ++j) sum += a[i][j];
        cout << "Average of row " << i << "=" << fixed << setprecision(2) << sum / m << endl;
    }
    return 0;
}
